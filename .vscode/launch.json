{
  "version": "0.2.0",
  "configurations": [

		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/server/index.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "browser-preview",
			"request": "launch",
			"name": "Browser Preview: Launch",
			"url": "http://localhost:3000"
		},
		{
			"type": "browser-preview",
			"request": "attach",
			"name": "Browser Preview: Attach"
		}

  ]
}